        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" x:Name="LekoviListBox" SelectionChanged="LekoviListBox_SelectionChanged" DisplayMemberPath="Ime" Margin="0,102,0,-102" />

    </Grid>
